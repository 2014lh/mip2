{
  "name": "mip-vuetify",
  "version": "0.0.1",
  "description": "mip vuetify components",
  "main": "dist/mip-vuetify.js",
  "scripts": {
    "dev": "node examples/server.js",
    "build": "node build/build.js",
    "uploadjs": "baidubce bos --put-object dist/mip-vuetify.js bos://assets/mip/projects/mip-vuetify.js",
    "uploadcss": "baidubce bos --put-object dist/mip-vuetify.css bos://assets/mip/projects/mip-vuetify.css",
    "deploy:dev": "npm run build && npm run uploadjs && npm run uploadcss",
    "lint": "eslint --ext .js **/* --ignore-pattern dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-builtin-classes": "^0.6.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "express-urlrewrite": "^1.2.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.0",
    "he": "^1.1.1",
    "http-proxy-middleware": "^0.18.0",
    "husky": "^0.14.3",
    "mini-css-extract-plugin": "^0.4.0",
    "ora": "^2.1.0",
    "path-to-regexp": "^1.7.0",
    "postcss-csso": "^3.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.7",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-less": "^0.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "serve-index": "^1.9.1",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglify-js": "^3.4.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.9.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "vuetify": "^1.0.19"
  }
}
