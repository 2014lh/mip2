{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "vue"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    ["transform-builtin-classes", {
      "globals": ["HTMLElement"]
    }],
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true
      }
    ]
  ],
  "ignore": ["dist/*.js"]
}
